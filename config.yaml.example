app:
  port: 8000

log:
  level: info

snapshot_path: data

tool_result:
  root_dir: data/tool_results
  inline_limit: 500
  preview_limit: 500
  fs_read_max_chars: 4000
  always_externalize_tools:
    - browser_html_content

agentrun:
  enabled: true
  template_name: your-browser-template-name
  account_id: ""
  region_id: cn-hangzhou
  access_key_id: ""
  access_key_secret: ""
  vnc_api_key: ""
  delete_on_shutdown: true

llm:
  model: qwen3.5-plus
  api_key: replace-with-your-key
  base_url: https://dashscope.aliyuncs.com/compatible-mode/v1
