app:
  port: 8000

log:
  level: info

snapshot_path: data

tool_result:
  root_dir: data/tool_results
  inline_limit: 500
  preview_limit: 500
  fs_read_max_chars: 4000
  always_externalize_tools:
    - chrome-devtools_take_snapshot
    - chrome-devtools_take_screenshot

llm:
  model: qwen3.5-plus
  api_key: 改成你的key
  base_url: https://dashscope.aliyuncs.com/compatible-mode/v1

integrations:
  daytona_enabled: true
  builtin_tools_enabled: false
  mcp_enabled: false

daytona:
  api_key: "" # daytone的api key
  target: "" # sandbox的区域 us / eu
  server_url: "" # daytone的host
  auto_stop_minutes: 15
  vnc_port: 6080
  vnc_url_ttl_seconds: 3600
